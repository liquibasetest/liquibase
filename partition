CREATE OR REPLACE PROCEDURE create_partitioned_external_table(
    inputname STRING,
    filelocation STRING
)
RETURNS STRING
LANGUAGE JAVASCRIPT
EXECUTE AS CALLER
AS
$$
try {
    var stage = '@mystage/';
    var file_format = 'my_parquet_format';
    var location = stage + filelocation + '/';

    return inputname; // Return inputname here
} catch (err) {
    return 'Error: ' + err.message;
}
$$;
